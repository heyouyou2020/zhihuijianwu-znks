\r\n          if (res.data) {\r\n            this.tableData = res.data.pageList\r\n            this.total = res.data.total\r\n            this.listLoading = false\r\n          }else {\r\n            this.$message.warning('暂无数据哦')\r\n            this.tableData = []\r\n            this.listLoading = false\r\n          }\r\n        }\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n  },\r\n  mounted() {\r\n  },\r\n  methods: {\r\n    handleFilter() {\r\n      this.listQuery.pageSize = 10\r\n      this.listQuery.page = 1\r\n      this.listLoading = true\r\n      this.$api.getWljiefangList({\r\n        visitorName: this.form.name,\r\n        startTime: this.form.time[0],\r\n        endTime: this.form.time[1],\r\n        optionValue: this.form.leibie,\r\n        state: this.form.state,\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n      }).then(res => {\r\n        console.log(res)\r\n        if (res.code === 0) {\r\n          if (res.data) {\r\n            this.tableData = res.data.pageList\r\n            this.total = res.data.total\r\n            this.listLoading = false\r\n          }else {\r\n            this.$message.warning('暂无数据哦')\r\n            this.tableData = []\r\n            this.listLoading = false\r\n          }\r\n        }\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    chongzhi() {\r\n      this.form.time = ['', '']\r\n      this.form.name = ''\r\n      this.form.state = ''\r\n      this.form.leibie = ''\r\n    },\r\n    getList() {\r\n      this.listLoading = false\r\n    },\r\n    handleClick(row) {\r\n      this.$router.push({\r\n        name: 'wljfxq',\r\n        params: {\r\n          id: row.id\r\n        }\r\n      })\r\n    },\r\n    refresh(val) {\r\n      this.listLoading = true\r\n      this.$api.getWljiefangList({\r\n        visitorName: this.form.name,\r\n        startTime: this.form.time[0],\r\n        endTime: this.form.time[1],\r\n        optionValue: this.form.leibie,\r\n      