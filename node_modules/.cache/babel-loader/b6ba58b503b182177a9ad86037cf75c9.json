       <el-button type=\"primary\" plain @click=\"restOff('listQuery')\">重置</el-button>\n          <el-button type=\"primary\" @click=\"onSearch\">查询</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n    <!--      表格statrt-->\n    <el-table\n      :key=\"tableKey\"\n      v-loading=\"listLoading\"\n      stripe\n      :data=\"tableData\"\n      style=\"width: 100%\"\n      :cell-style=\"changeCellStyle\"\n      highlight-current-row\n    >\n      <el-table-column\n        prop=\"ajmc\"\n        label=\"案件名称\"\n        width=\"200\"\n        cell-class-name=\"colorB\"\n        :show-overflow-tooltip=\"true\"\n      />\n      <el-table-column\n        prop=\"bmsah\"\n        width=\"200\"\n        label=\"部门受案号\"\n      />\n      <el-table-column\n        prop=\"cbbm\"\n        label=\"承办部门\"\n        width=\"120\"\n      />\n      <el-table-column\n        prop=\"cbdw\"\n        label=\"承办单位\"\n        width=\"120\"\n      />\n      <el-table-column\n        prop=\"wtr\"\n        label=\"委托人\"\n        width=\"110\"\n      />\n      <el-table-column\n        prop=\"wtrq\"\n        label=\"委托日期\"\n        width=\"120\"\n      />\n      <el-table-column\n        prop=\"flfs\"\n        label=\"分流方式\"\n      />\n      <el-table-column\n        prop=\"xtzt\"\n        label=\"协同状态\"\n        width=\"120\"\n      >\n        <template slot-scope=\"scope\">\n          <span v-for=\"item in xtztItem\" :key=\"item.key\">\n            <span v-if=\"scope.row.xtzt==item.key\" :style=\"{color: item.color}\">{{ item.name }}</span>\n          </span>\n        </template>\n      </el-table-column>\n      <el-table-column\n        label=\"操作\"\n        width=\"200\"\n      >\n        <template slot-scope=\"scope\">\n          <el-button v-if=\"scope.row.xtzt=='1'\" type=\"text\" size=\"small\" @click=\"trustItem(scope.row)\">协同委托</el-button>\n          <el-button v-if=\"scope.row.xtzt=='2'\" type=\"text\" size=\"small\" @click=\"checkItem(scope.row)\">协同审查</el-button>\n          <el-button v-if=\"scope.row.xtzt=='3' || scope.row.xtzt=='2'\" type=\"text\" size=\"small\" @click=\"lookItem(scope.row)\">查看</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div class=\"pageBox\">\n      <pagination\n        v-show=\"total > 0\"\n        :total=\"total\"\n        :page.sync=\"listQuery.currentPage\"\n        :limit.sync=\"listQuery.pageSize\"\n        @pagination=\"refresh\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport Pagination from '@/components/Pagination/index'\nexport default {\n  name: 'Dashboard',\n  components: { Pagination },\n  data() {\n    return {\n      listQuery: {\n        status: 3,\n        userid: 1,\n        pageSize: 10,\n        currentPage: 1\n      },\n      tableKey: 0,\n      tableData: [],\n      listLoading: true,\n      total: 100,\n      xtztItem: [\n        {\n          key: 2,\n          name: '已接受',\n          color: '#00CC00'\n        },\n        {\n          key: 3,\n          name: '已拒绝',\n          color: '#FF0000'\n        },\n        {\n          kay: 0,\n          name: '未协同',\n          color: '#999999'\n        },\n        {\n          key: 1,\n          name: '待接收',\n          color: '#999999'\n        }\n      ],\n      pickerOptions0: {\n        disabledDate(time) {\n          return time.getTime() > Date.now()\n        }\n      }\n    }\n  },\n  mounted() {\n    this.getList()\n  },\n  methods: {\n    selectDate: function() {\n      const dateTime = this.listQuery.wtrq\n      if (dateTime == null) {\n        this.listQuery.wtkssj = ''\n        this.listQuery.wtjssj = ''\n      } else {\n        const startTime = this.moment(dateTime[0]).format('YYYY-MM-DD')\n        const endTime = this.moment(dateTime[1]).format('YYYY-MM-DD')\n        this.listQuery.wtkssj = startTime\n        this.listQuery.wtjssj = endTime\n      }\n    },\n    onSearch: function() {\n      this.listQuery.currentPage = 1\n      this.getList()\n    },\n    restOff: function(listQuery) {\n      this.$refs[listQuery].resetFields()\n      this.listQuery.wtkssj = ''\n      this.listQuery.wtjssj = ''\n      this.getList()\n    },\n    changeCellStyle({ row, columnIndex }) {\n      if (columnIndex === 0) {\n        return 'color: #4084ef'\n      }\n    },\n    getList() {\n      this.listLoading = true// true\n      this.$api.scwtPageList(this.listQuery).then(response => {\n        if (response.code === 200) {\n          this.tableData = response.data.list\n          this.total = response.data.total\n          this.listLoading = false\n        } else {\n          this.$message({\n            message: response.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    checkItem: function(item) {\n      this.$router.push({ path: '/xtsc/xtsc', query: { xsInfoId: item.id }})\n    },\n    trustItem: function(item) {\n      this.$router.push({ path: '/xtsc/xtck', query: { xsInfoId: item.id, zt: 0 }})\n    },\n    lookItem(item) {\n      this.$router.push({ path: '/xtsc/xtck', query: { xsInfoId: item.id, zt: 1 }})\n    },\n    refresh() {\n      this.getList()\n    }\n\n  }\n}\n</script>\n"],"sourceRoot":"src/views/zhscjd/xtsc"}]}