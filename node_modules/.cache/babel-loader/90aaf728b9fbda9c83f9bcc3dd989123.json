nd:#1890ff;color:white;\"\r\n              @click=\"handleAdd\"\r\n            >\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <!--      表格statrt-->\r\n      <el-table\r\n        v-loading=\"listLoading\"\r\n        class=\"table-hidden-cell\"\r\n        :data=\"tableData\"\r\n        stripe\r\n        highlight-current-row\r\n      >\r\n        <el-table-column\r\n          v-for=\"item in tableHeader\"\r\n          :key=\"item.filed\"\r\n          :prop=\"item.filed\"\r\n          :label=\"item.title\"\r\n          :width=\"item.width\"\r\n          sortable\r\n          :show-overflow-tooltip=\"true\"\r\n          :cell-class-name=\"item.colorB\"\r\n        />\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" size=\"small\" @click=\"handleClick(scope.row)\">查看</el-button>\r\n            <!-- <el-button type=\"text\" size=\"small\" @click=\"handleChange(scope.row)\">修改</el-button>\r\n            <el-button type=\"text\" size=\"small\" @click=\"handleDle(scope.row)\">删除</el-button> -->\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页start-->\r\n      <div class=\"pageBox\">\r\n        <pagination\r\n          v-show=\"total > 0\"\r\n          :total=\"total\"\r\n          :page.sync=\"listQuery.page\"\r\n          :limit.sync=\"listQuery.pageSize\"\r\n          @pagination=\"refresh\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Pagination from '@/components/Pagination/index'\r\nexport default {\r\n  name: 'ReviewArrest',\r\n  components: { Pagination },\r\n  data() {\r\n    return {\r\n      anjianinfo: [],\r\n      infoPushVisible: false,\r\n      bianGenInfo: {\r\n        czrymc: '',\r\n        bgrq: '',\r\n        optContent: '',\r\n      },\r\n      form: {\r\n        leibie: '',\r\n        time: ['', ''],\r\n        name: '',\r\n      },\r\n      // tableData: [],