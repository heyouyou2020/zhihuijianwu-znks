div>\n          <p class=\"mb5 pt20\"><span>{{ progressData0.name }}</span><span class=\"fr pr50 custom\">{{ progressData0.value }}</span></p>\n          <el-progress :percentage=\"progressData0.value\" :stroke-width=\"15\" :color=\"custom\" />\n          <p class=\"mb5 mt30\"><span>{{ progressData1.name }}</span><span class=\"fr pr50 custom2\">{{ progressData1.value }}</span></p>\n          <el-progress :percentage=\"progressData0.value\" :stroke-width=\"15\" :color=\"custom2\" />\n        </el-col>\n      </el-row>\n    </div>\n    <!--    <scqk />-->\n    <el-row class=\"mt20\">\n      <h4>审查情况统计</h4>\n    </el-row>\n    <div class=\"partBox\">\n      <el-row :gutter=\"40\">\n        <el-col :span=\"10\" class=\"dashed-right-line mb15\">\n          <mapChart id=\"map-chart\" width=\"100%\" height=\"380px\" :map-data=\"mapData\" @clickMap=\"clickTableMap\" />\n          <p class=\"base-desc-title\"><span>-审查情况地区展示-</span></p>\n        </el-col>\n        <el-col :span=\"13\" class=\"mb15\" style=\"padding-right: 0\">\n          <div class=\"el-row mb30\"><span class=\"lineStep\" /> <span>各城市委托统计</span></div>\n          <el-table\n            :data=\"tableData\"\n            style=\"width: 100%\"\n            class=\"myTable\"\n          >\n            <el-table-column\n              type=\"index\"\n              label=\"序号\"\n              :index=\"indexMethod\"\n              width=\"60\"\n            />\n            <el-table-column\n              prop=\"name\"\n              label=\"城市\"\n              width=\"90\"\n            />\n            <el-table-column\n              prop=\"zs\"\n              label=\"审查总数\"\n              width=\"100\"\n            />\n            <el-table-column\n              prop=\"fy\"\n              label=\"法医病理鉴定\"\n              width=\"110\"\n            />\n            <el-table-column\n              prop=\"ws\"\n              label=\"文书鉴定\"\n              width=\"90\"\n            />\n            <el-table-column\n              prop=\"kj\"\n              label=\"会计司法鉴定\"\n              width=\"110\"\n            />\n            <el-table-column\n              prop=\"qt\"\n              label=\"其他鉴定\"\n            />\n          </el-table>\n          <div class=\"pageBox\">\n            <pagination\n              v-show=\"total > 0\"\n              :total=\"total\"\n              :page.sync=\"tableList.currentPage\"\n              :limit.sync=\"tableList.pageSize\"\n              @pagination=\"refresh\"\n            />\n          </div>\n        </el-col>\n      </el-row>\n    </div>\n    <el-row class=\"mt20\">\n      <h4>资源库使用情况统计</h4>\n    </el-row>\n    <div class=\"partBox\">\n      <el-row>\n        <div class=\"el-row mb30\"><span class=\"lineStep\" /> <span>各城市使用资源库统计</span></div>\n        <gradient-bar id=\"grad-bar\" :chart-data=\"barData\" :x-axis-data=\"xAxisData\" :is-show-legend=\"true\" :color-arr=\"colorArr\" width=\"100%\" height=\"380px\" :title-text=\"titleText\" />\n      </el-row>\n    </div>\n    <el-row class=\"mt20\">\n      <h4>期限预警分析</h4>\n    </el-row>\n    <div class=\"partBox\">\n      <el-row>\n        <div class=\"el-row mb30\"><span class=\"lineStep\" /> <span>各城市期限预警分析</span></div>\n        <LinMarker id=\"line-marker\" :chart-data=\"lineData\" :x-axis-data=\"xAxisDataLline\" :is-show-legend=\"true\" width=\"100%\" height=\"380px\" />\n      </el-row>\n    </div>\n    <el-row class=\"mt20\">\n      <h4>资源调用分析</h4>\n    </el-row>\n    <div class=\"partBox\">\n      <el-row>\n        <div class=\"el-row mb30\"><span class=\"lineStep\" /> <span>各城市调用资源库频率</span></div>\n        <gradient-bar id=\"grad-bar2\" :chart-data=\"barData2\" :x-axis-data=\"xAxisData2\" :is-show-legend=\"true\" :color-arr=\"colorArr2\" width=\"100%\" height=\"380px\" :title-text=\"titleText2\" />\n      </el-row>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PieCharts from '@/components/Charts/PieEcharts'\nimport GradientBar from '@/components/Charts/barEcharts'\nimport LinMarker from '@/components/Charts/LineMarker'\nimport mapChart from '@/components/Charts/mapEcharts'\nimport Pagination from '@/components/Pagination/index'\n// import scqk from './components/scqk'\nexport default {\n  name: 'Dashboard',\n  components: { PieCharts, mapChart, GradientBar, LinMarker, Pagination },\n  data() {\n    return {\n      listQuery: {},\n      pieData: [],\n      progressData0: {},\n      progressData1: {},\n      custom: '#5ce4bd',\n      custom2: '#397ef0',\n      barData: [],\n      xAxisData: [],\n      xAxisData2: [],\n      colorArr: [],\n      titleText: '',\n      lineData: [],\n      xAxisDataLline: [],\n      barData2: [],\n      colorArr2: [],\n      titleText2: '',\n      mapData: [],\n      tableData: [\n        {\n          name: '济南',\n          address: '466'\n        }\n      ],\n      tableList: {\n        pageSize: 5,\n        currentPage: 1\n      },\n      total: 0,\n      pickerOptions0: {\n        disabledDate(time) {\n          return time.getTime() > Date.now()\n        }\n      }\n    }\n  },\n  created() {\n    this.titleText = '数量'\n    this.colorArr = [{ top: '#4084ef', bottom: 'rgba(64, 132, 239, 0.1)' }, { top: '#37d6b7', bottom: 'rgba(55, 214, 183, 0.1)' }, { top: '#ff7d70', bottom: 'rgba(255, 125, 112, 0.1)' }, { top: '#ffab32', bottom: 'rgba(255, 171, 50, 0.1)' }, { top: '#4185ef', bottom: 'rgba(65, 133, 239, 0.1)' }, { top: '#38d7b7', bottom: 'rgba(56, 215, 183, 0.1)' }]\n    this.colorArr2 = [{ top: '#ffb541', bottom: 'rgba(255, 181, 65, 0.1)' }]\n    this.titleText2 = '调用频率（次/min）'\n  },\n  mounted() {\n    this.initData()\n  },\n  methods: {\n    filterArr: function(objList, key, findName) {\n      // debugger;\n      findName = findName || 'SeriesName'\n      return objList.filter(function(item) {\n        return key.indexOf(item[findName]) !== -1\n      })\n    },\n    initData() {\n      this.getsyqk()\n      this.getzydy()\n      this.getwtfl()\n      this.getwtfliu()\n      this.getqxyj()\n      this.getMapData()\n      this.getTableMap()\n    },\n    selectDate: function() {\n      const dateTime = this.listQuery.fxsj\n      if (dateTime == null) {\n        this.listQuery.kssj = ''\n        this.listQuery.jssj = ''\n      } else {\n        const startTime = this.moment(dateTime[0]).format('YYYY-MM-DD')\n        const endTime = this.moment(dateTime[1]).format('YYYY-MM-DD')\n        this.listQuery.kssj = startTime\n        this.listQuery.jssj = endTime\n      }\n    },\n    onSearch: function() {\n      this.tableList.currentPage = 1\n      this.initData()\n    },\n    restOff: function(listQuery) {\n      this.$refs[listQuery].resetFields()\n      this.listQuery.slksrq = ''\n      this.listQuery.sljsrq = ''\n      this.initData()\n    },\n    getsyqk() {\n      this.$api.queryZyksyqk(this.listQuery).then(response => {\n        if (response.code === 200) {\n          this.barData = []\n          this.barData.push(response.data.jdalk)\n          this.barData.push(response.data.jdfvfg)\n          this.barData.push(response.data.yxztd)\n          this.barData.push(response.data.zjjs)\n          this.xAxisData = response.data.citylist\n        } else {\n          this.$message({\n            message: response.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    getzydy() {\n      this.$api.queryZydyfx(this.listQuery).then(response => {\n        if (response.code === 200) {\n          const list = response.data.result\n          this.barData2 = []\n          this.barData2.push({ name: '调用频率', data: list })\n          this.xAxisData2 = response.data.citylist\n        } else {\n          this.$message({\n            message: response.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    getwtfl() {\n      this.$api.getWtCount(this.listQuery).then(response => {\n        if (response.length > 0) {\n          this.pieData = response\n        } else {\n          this.$message({\n            message: '接口报错',\n            type: 'warning'\n          })\n        }\n      })\n    },\n    getwtfliu() {\n      this.$api.getFlCount(this.listQuery).then(response => {\n        if (response.length > 0) {\n          const progressData = response\n          this.progressData0 = progressData[0]\n          this.progressData1 = progressData[1]\n        } else {\n          this.$message({\n            message: '接口报错',\n            type: 'warning'\n          })\n        }\n      })\n    },\n    getqxyj() {\n      this.$api.getQxyjfxCount(this.listQuery).then(response => {\n        if (response.barData.length > 0) {\n          this.lineData = response.barData\n          this.xAxisDataLline = response.dw\n        } else {\n          this.$message({\n            message: '接口报错',\n            type: 'warning'\n          })\n        }\n      })\n    },\n    indexMethod(index) {\n      return index\n    },\n    getMapData() {\n      this.$api.getWtflCount(this.listQuery).then(response => {\n        if (response.value.length > 0) {\n          this.mapData = response.value\n        } else {\n          this.$message({\n            message: '接口报错',\n            type: 'warning'\n          })\n        }\n      })\n    },\n    clickTableMap(item) {\n      const params = {\n        kssj: this.listQuery.kssj,\n        jssj: this.listQuery.jssj,\n        pageSize: this.tableList.pageSize,\n        currentPage: this.tableList.currentPage,\n        dwbm: item.data.code\n      }\n      this.$api.getWtListCountFl(params).then(response => {\n        if (response.code === 200) {\n          this.tableData = response.data.list\n        } else {\n          this.$message({\n            message: response.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    getTableMap() {\n      const params = {\n        kssj: this.listQuery.kssj,\n        jssj: this.listQuery.jssj,\n        pageSize: this.tableList.pageSize,\n        currentPage: this.tableList.currentPage,\n        dwbm: ''\n      }\n      this.$api.getWtListCountFl(params).then(response => {\n        if (response.code === 200) {\n          this.tableData = response.data.list\n        } else {\n          this.$message({\n            message: response.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    refresh() {\n      this.clickTableMap()\n    },\n  }\n}\n</script>\n<style>\n    .partBox{\n        padding: 15px;\n        border: 1px solid #a9c4df;\n        color: #4f5e7b;\n    }\n    .el-progress__text{\n        display: none;\n    }\n    .custom{\n        color: #5ce4bd;\n    }\n    .custom2{\n        color:  #397ef0;\n    }\n    .myTable{\n        border-left: solid 1px #ceddeb;\n        border-right: solid 1px #ceddeb;\n        color: #4a505a;\n    }\n    .el-table/deep/ th>.cell{\n        border-right: none;\n    }\n    .el-table/deep/ thead th{\n        background-color: #f1f6fb;\n    }\n    .base-desc-title{\n        text-align: center;\n        margin-top: 10px;\n    }\n</style>\n"],"sourceRoot":"src/views/zhscjd/sczyk/tjyjc"}]}