" :model=\"xsItemInfo\" class=\"mt20\">\n        <table class=\"renderTable\">\n          <tbody>\n            <tr>\n              <td>部门受案号：</td>\n              <td>{{ xsItemInfo.bmsah }}</td>\n              <td>案件类别：</td>\n              <td>\n                {{ xsItemInfo.ajlb }}\n              </td>\n            </tr>\n            <tr>\n              <td>承办部门：</td>\n              <td>{{ xsItemInfo.cbbm }}</td>\n              <td>承办单位：</td>\n              <td>\n                {{ xsItemInfo.cbdw }}\n              </td>\n            </tr>\n            <tr>\n              <td>委托人：</td>\n              <td>{{ xsItemInfo.wtrname }}</td>\n              <td>委托日期：</td>\n              <td>\n                {{ xsItemInfo.wtrq }}\n              </td>\n            </tr>\n            <tr>\n              <td>鉴定类型：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.jdlx }}\n              </td>\n            </tr>\n            <tr>\n              <td>简要案情：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.jyaq }}\n              </td>\n            </tr>\n            <tr>\n              <td>委托要求：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.wtyq }}\n              </td>\n            </tr>\n            <tr>\n              <td>资料上传：</td>\n              <td colspan=\"3\">\n                <ul v-if=\"zlList.length>0\" class=\"uploadShow\">\n                  <li  v-for=\"(item,i) in zlList\" :key=\"i\">\n                    <span class=\"fr ml10\" style=\"cursor: pointer\" @click=\"down(item)\"><img src=\"@/icons/img/down.png\" height=\"12\" width=\"12\"></span>\n                    <span v-if=\"item.type=='pic'\"><img src=\"@/icons/img/pic.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else-if=\"item.type=='audio'\"><img src=\"@/icons/img/audio.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else-if=\"item.type=='video'\"><img src=\"@/icons/img/video.png\" height=\"16\" width=\"1