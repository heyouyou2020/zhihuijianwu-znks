\n            v-model=\"listQuery.xm\"\n            placeholder=\"\"\n            class=\"filter-item\"\n            clearable\n            maxlength=\"30\"\n          />\n        </el-form-item>\n        <el-form-item label=\"专业领域\" prop=\"zyly\">\n          <el-select v-model=\"listQuery.zyly\" placeholder=\"请选择\" value-key=\"value\">\n            <el-option label=\"全部\" value=\"\" />\n            <el-option\n              v-for=\"item in zylyOption\"\n              :key=\"item.value\"\n              :label=\"item.label\"\n              :value=\"item.value\"\n            />\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"所属单位\" prop=\"ssdw\">\n          <el-input\n            v-model=\"listQuery.ssdw\"\n            placeholder=\"\"\n            class=\"filter-item\"\n            clearable\n            maxlength=\"30\"\n          />\n        </el-form-item>\n        <el-form-item>\n          <el-button type=\"primary\" plain @click=\"restOff('listQuery')\">重置</el-button>\n          <el-button type=\"primary\" @click=\"onSearch\">查询</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n    <el-row class=\"mb10\">\n      <el-button\n        plain\n        class=\"btn-noBg fr\"\n        style=\"margin-left: 10px\"\n        @click=\"jumpAdd\"\n      >\n        <img src=\"@/icons/img/add.png\" height=\"15\" width=\"15\" style=\"vertical-align: middle\">\n        添加技术人员\n      </el-button>\n    </el-row>\n    <!--      表格statrt-->\n    <el-table\n      :key=\"tableKey\"\n      v-loading=\"listLoading\"\n      stripe\n      :data=\"tableData\"\n      style=\"width: 100%\"\n      :cell-style=\"changeCellStyle\"\n      highlight-current-row\n    >\n      <el-table-column\n        prop=\"xm\"\n        label=\"技术人员姓名\"\n        cell-class-name=\"colorB\"\n        :show-overflow-tooltip=\"true\"\n      />\n      <el-table-column\n        prop=\"xl\"\n        label=\"学历\"\n      />\n      <el-table-column\n        prop=\"ssbm\"\n        label=\"所属部门\"\n        width=\"120\"\n      />\n      <el-table-column\n        prop=\"ssdw\"\n        label=\"所属单位\"\n        width=\"120\"\n      />\n      <el-table-column\n        prop=\"zylymc\"\n        label=\"专业领域\"\n        width=\"180\"\n      />\n      <el-table-column\n        prop=\"tc\"\n        label=\"特长\"\n        width=\"180\"\n      />\n      <el-table-column\n        label=\"操作\"\n        width=\"200\"\n      >\n        <template slot-scope=\"scope\">\n          <el-button type=\"text\" size=\"small\" @click=\"checkItem(scope.row)\">查看</el-button>\n          <el-button type=\"text\" size=\"small\" @click=\"editItem(scope.row)\">编辑</el-button>\n          <el-button type=\"text\" size=\"small\" @click=\"deleteItem(scope.row)\">删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div class=\"pageBox\">\n      <pagination\n        v-show=\"total > 0\"\n        :total=\"total\"\n        :page.sync=\"listQuery.currentPage\"\n        :limit.sync=\"listQuery.pageSize\"\n        @pagination=\"refresh\"\n      />\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport Pagination from '@/components/Pagination/index'\nexport default {\n  name: 'Dashboard',\n  components: { Pagination },\n  data() {\n    return {\n      listQuery: {\n        lb: '1',\n        pageSize: 10,\n        currentPage: 1\n      },\n      zylyOption: [],\n      tableKey: 0,\n      tableData: [],\n      listLoading: true,\n      total: 100\n    }\n  },\n  mounted() {\n    this.getZyly()\n    this.getList()\n  },\n  methods: {\n    getZyly: function() {\n      this.$api.getCode('zyly').then(response => {\n        if (response.code === 200) {\n          this.zylyOption = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    onSearch: function() {\n      this.listQuery.currentPage = 1\n      this.getList()\n    },\n    restOff: function(listQuery) {\n      this.$refs[listQuery].resetFields()\n      this.getList()\n    },\n    changeCellStyle({ row, columnIndex }) {\n      if (columnIndex === 0) {\n        return 'color: #4084ef'\n      }\n    },\n    getList() {\n      this.listLoading = true// true\n      this.$api.queryScrylist(this.listQuery).then(response => {\n        if (response.code === 200) {\n          this.tableData = response.data.list\n          this.total = response.data.total\n          this.listLoading = false\n        } else {\n          this.$message({\n            message: response.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    checkItem: function(item) {\n      this.$router.push({ path: '/jcjsryk/view', query: { itemId: item.rybh }})\n    },\n    editItem: function(item) {\n      this.$router.push({ path: '/jcjsryk/addEditor', query: { itemId: item.rybh }})\n    },\n    deleteItem: function(item) {\n      this.$confirm('此操作将永久删除, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$api.delScryxx(item.rybh).then(response => {\n          if (response.code === 200) {\n            this.getList()\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            })\n          }\n        })\n      }).catch(() => {\n        // this.$message({\n        //   type: 'info',\n        //   message: '已取消删除'\n        // })\n      })\n    },\n    refresh() {\n      this.getList()\n    },\n    jumpAdd: function() {\n      this.$router.push({ path: '/jcjsryk/addEditor', query: { status: 'add' }})\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/zhscjd/sczyk/jcjsryk"}]}