this.getXsInfo(this.xsItem)\n    this.getXsfw()\n    this.getLaqk()\n    this.getFxyj()\n    this.getWfsx()\n  },\n  methods: {\n    getXsInfo(xsbh) {\n      this.$api.queryXsxq(xsbh).then(response => {\n        if (response.code === 200) {\n          this.xsForm = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    getXsfw: function() {\n      this.$api.queryXsfw().then(response => {\n        if (response.code === 200) {\n          this.xsfwOption = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    getLaqk: function() {\n      this.$api.queryZcqk().then(response => {\n        if (response.code === 200) {\n          this.laqkOption = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    getFxyj: function() {\n      this.$api.queryFxyj().then(response => {\n        if (response.code === 200) {\n          this.fxyjOption = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    getWfsx: function() {\n      this.$api.queryWfsx().then(response => {\n        if (response.code === 200) {\n          this.wfsxOption = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    editSave: function(xsForm) {\n      xsForm.lb = '1'\n      xsForm.xszt = '3'\n      this.$refs[xsForm].validate((valid) => {\n        if (valid) {\n          if (new Date(this.xsForm.xsfsrq) > new Date(this.xsForm.xsslrq)) {\n            this.$message.error('结束时间不能小于开始时间')\n            return false\n          }\n          this.$api.updateXsinfo(this.xsForm).then(response => {\n            if (response.code === 200) {\n              this.$message({\n                message: '保存成功',\n                type: 'success'\n              })\n              this.$router.push('/lajdfzbc/index')\n            } else {\n              this.$message.error(response.message)\n            }\n          })\n        }\n      })\n    },\n    goBack: function() {\n      this.$router.back(-1)\n    }\n  }\n}\n</script>\n<style scoped>\n   .el-dialog__body /deep/{\n    height: 500px !important;\n  }\n</style>\n\n"],"sourceRoot":"src/views/xsjcjd/lajdfz/bcxs"}]}