     */\r\n    checkBtn(row) {\r\n      row.c = !row.c\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang='scss' scoped>\r\n.manageList{\r\n  .reset{\r\n    background: #ebf3fe;\r\n    color: #5c7ab0;\r\n    border-color: #ebf3fe;\r\n    &:hover{\r\n      background: #c4dcf4;\r\n      color: #5c7ab0;\r\n      border-color: #cbdbf3;\r\n    }\r\n  }\r\n  .batchOperate{\r\n    button{\r\n      padding: 8px 33px 8px 45px;\r\n      &:first-child{\r\n        background: url('../../../../icons/img/add.png') no-repeat left 13px center;\r\n      }\r\n      &:last-child{\r\n        padding: 8px 33px;\r\n        background: none;\r\n        color: #1b9dff;\r\n        border-color: #1b9dff;\r\n      }\r\n    }\r\n  }\r\n  .table-hidden-cell{\r\n    .el-button--medium{\r\n      padding: 0;\r\n    }\r\n  }\r\n  .tabOC{\r\n    display: inline-block;\r\n    width: 33px;\r\n    height: 16px;\r\n    cursor: pointer;\r\n    background: url('../../../../icons/img/chenckClose.png') no-repeat;\r\n    &.open{\r\n      background: url('../../../../icons/img/chenckOpen.png') no-repeat;\r\n    }\r\n  }\r\n}\r\n</style>\r\n<style lang='scss'>\r\n.message-manage{\r\n  .el-message-box__title{\r\n    color: #4084f0;\r\n  }\r\n  .el-message-box__header{\r\n    padding-bottom: 21px;\r\n  }\r\n  .el-icon-warning:before{\r\n    content: '';\r\n    display: inline-block;\r\n    width: 52px;\r\n    height: 52px;\r\n    background: url('../../../../icons/img/tishi.png') no-repeat;\r\n    background-size: 100%;\r\n  }\r\n  .el-message-box__status+.el-message-box__message{\r\n    padding-left: 66px;\r\n  }\r\n  .el-message-box__btns{\r\n    button{\r\n      width: 98px;\r\n      height: 34px;\r\n      float: right;\r\n      &:first-child{\r\n        margin-left: 10px;\r\n        color: #5c7ab0;\r\n        background: #c4dcf4;\r\n        &:hover{\r\n          background: #cce4fc;\r\n        }\r\n      }\r\n      span{\r\n        font-size: 16px;\r\n      }\r\n    }\r\n  }\r\n}\r\n.manageList{\r\n  .el-form-