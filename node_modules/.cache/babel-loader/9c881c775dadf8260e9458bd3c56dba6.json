           <td>承办部门：</td>\n              <td>{{ xsItemInfo.cbbm }}</td>\n              <td>承办单位：</td>\n              <td>\n                {{ xsItemInfo.cbdw }}\n              </td>\n            </tr>\n            <tr>\n              <td>委托人：</td>\n              <td>{{ xsItemInfo.wtr }}</td>\n              <td>委托日期：</td>\n              <td>\n                {{ xsItemInfo.wtrq }}\n              </td>\n            </tr>\n            <tr>\n              <td>鉴定类型：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.jdlx }}\n              </td>\n            </tr>\n            <tr>\n              <td>简要案情：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.jyaq }}\n              </td>\n            </tr>\n            <tr>\n              <td>委托要求：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.wtyq }}\n              </td>\n            </tr>\n            <tr>\n              <td>资料上传：</td>\n              <td colspan=\"3\">\n                <ul v-if=\"zlList.length>0\" class=\"uploadShow\">\n                  <li v-for=\"(item,i) in zlList\" :key=\"i\">\n                    <span class=\"fr ml10\" style=\"cursor: pointer\" @click=\"down(item)\"><img src=\"@/icons/img/down.png\" height=\"12\" width=\"12\"></span>\n                    <span v-if=\"item.type=='pic'\"><img src=\"@/icons/img/pic.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else-if=\"item.type=='audio'\"><img src=\"@/icons/img/audio.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else-if=\"item.type=='video'\"><img src=\"@/icons/img/video.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else><img src=\"@/icons/img/wd.png\" height=\"16\" width=\"14\"></span>\n                    <span>{{ item.name }}</span>\n                  </li>\n                </ul>\n              </td>\n            </tr>\n            <tr>\n              <td>备注：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.bz }}\n              </td>\n            </tr>\n            <tr v-if=\"zt==0\">\n              <td>协同原因：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.xtyy }}\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </el-form>\n      <el-row>\n        <div v-if=\"zt==0\" class=\"mt50 fr\">\n          <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"$router.back(-1)\">取消</el-button>\n        </div>\n        <div v-if=\"zt==1\" class=\"mt50 fr\">\n          <el-button type=\"primary\" @click=\"agree\">同意</el-button>\n          <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"reject\">拒绝</el-button>\n        </div>\n      </el-row>\n\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      xsbh: '',\n      zt: '',\n      xsItemInfo: {},\n      zlList: [],\n      downSrc: ''\n    }\n  },\n  mounted() {\n    this.xsbh = this.$route.query.xsInfoId\n    this.zt = this.$route.query.zt\n    this.getXsInfo(this.xsbh)\n  },\n  methods: {\n    getXsInfo(xsbh) {\n      const param = {\n        wtid: xsbh,\n        scrid: JSON.parse(window.localStorage.getItem('userInfo')).id,\n        sfxtr: 1\n      }\n      this.$api.getScwtByIdToScXt(param).then(response => {\n        if (response.code === 200) {\n          this.xsItemInfo = response.data\n          this.zlList = this.xsItemInfo.zls\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    down(item) {\n      this.$api.downloadWj(item.id).then(response => {\n        if (response.code === 200) {\n          this.downSrc = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    agree() {\n      const param = {\n        xtzt: 2,\n        id: this.xsItemInfo.id,\n        wtid: this.xsbh,\n        scrid: JSON.parse(window.localStorage.getItem('userInfo')).id,\n        flid: this.xsItemInfo.flid\n      }\n      this.$confirm('确认同意此审查委托吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$api.updateAjflXtzt(param).then(response => {\n          if (response.code === 200) {\n            this.$message({\n              type: 'success',\n              message: '操作成功!'\n            })\n            this.$router.push('/xtsc')\n          }\n        })\n      }).catch(() => {\n        // this.$message({\n        //   type: 'info',\n        //   message: '已取消删除'\n        // })\n      })\n    },\n    reject() {\n      const param = {\n        xtzt: 3,\n        id: this.xsItemInfo.id,\n        wtid: this.xsbh,\n        scrid: JSON.parse(window.localStorage.getItem('userInfo')).id,\n        flid: this.xsItemInfo.flid\n      }\n      this.$confirm('确认拒绝此审查委托吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$api.updateAjflXtzt(param).then(response => {\n          if (response.code === 200) {\n            this.$message({\n              type: 'success',\n              message: '操作成功!'\n            })\n            this.$router.push('/xtsc')\n          }\n        })\n      }).catch(() => {\n        // this.$message({\n        //   type: 'info',\n        //   message: '已取消删除'\n        // })\n      })\n    }\n  }\n}\n</script>\n\n"],"sourceRoot":"src/views/zhscjd/xtsc"}]}