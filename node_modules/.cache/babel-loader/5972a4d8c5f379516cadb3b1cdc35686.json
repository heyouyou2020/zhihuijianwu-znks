mit.sync=\"listQuery.pageSize\"\r\n          @pagination=\"refresh\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <!-- 办案情况弹框 -->\r\n    <el-dialog\r\n      v-if=\"caseShow\"\r\n      :show-close=\"isShowClose\"\r\n      class=\"base-dialog\"\r\n      :title=\"caseTitle\"\r\n      :visible.sync=\"caseVisible\"\r\n      top=\"26vh\"\r\n      width=\"1200px\"\r\n    >\r\n      <div class=\"bese-card p20\">\r\n        <el-row :gutter=\"60\">\r\n          <el-col :span=\"14\" class=\"dashed-right-line\">\r\n            <p class=\"base-desc-title\"><img src=\"@/icons/img/title-arrow.png\"><span>检察官办案量按照时间维度统计</span></p>\r\n            <gradient-bar id=\"grad-bar\" :chartData=\"barData\" :xAxisData=\"xAxisData\" width=\"100%\" height=\"280px\" @clickBar=\"clickBar\" />\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <p class=\"base-desc-title\"><img src=\"@/icons/img/title-arrow.png\"><span>检察官办案量按照已办/在办维度统计</span></p>\r\n            <pie-charts id=\"pie-chart\" width=\"100%\" height=\"280px\" :pie-data=\"pieData\" unit=\"件\" />\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <div slot=\"footer\">\r\n        <el-button type=\"primary\" @click=\"displayPop\">确定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Pagination from '@/components/Pagination/index'\r\nimport GradientBar from '@/components/Charts/GradientBar'\r\nimport PieCharts from '@/components/Charts/PieCharts'\r\nexport default {\r\n  name: 'ReviewArrest',\r\n  components: { Pagination, GradientBar, PieCharts },\r\n  data() {\r\n    return {\r\n      caseShow: false,\r\n      isShowClose: false,\r\n      allBuMen: [],\r\n      lunAnZu: [],\r\n      form: {\r\n        groupName: '',\r\n        jg: '',\r\n        time: '',\r\n        sjMan: '',\r\n        bmbm: '',\r\n        startDate: '',\r\n        endDate: ''\r\n      },\r\n      barData: [\r\n        {\r\n          name: '案件数',\r\n          type: 'bar',\r\n          data: [\r\n          ]\r\n        },\r\n      ],\r\n      pieData: [],\r\n      xAxisData: [],\r\n      // tableData: [],\r\n      listQuery: {\r\n        page: 1,\r\n        pageSize: 20,\r\n        keyWord: ''\r\n      },\r\n      tableHeader: [\r\n        { filed: 'rymc', title: '承办人', colorB: 'colorB', width: '200' },\r\n        { filed: 'sfzw', title: '在位状态' },\r\n        { filed: 'bmmc', title: '轮案组' },\r\n        { filed: 'lazmc', title: '所属部门' },\r\n      ],\r\n      tableData: [\r\n      ],\r\n      listLoading: true,\r\n      total: 0,\r\n      caseVisible: false,\r\n      caseTitle: '检察官办案情况统计分析'\r\n    }\r\n  },\r\n  created() {\r\n    this.getTime()\r\n    this.$api.getLunAllBuMen({ dwbm: '980000' })\r\n      .then(res => {\r\n        console.log(res)\r\n        this.allBuMen = res.data\r\n      })\r\n  },\r\n  mounted() {\r\n    this.getList()\r\n    console.log(this.xAxisData)\r\n    console.log(this.barData)\r\n  },\r\n  methods: {\r\n    change(val) {\r\n      console.log(this.form.bmbm)\r\n      this.form.groupName = ''\r\n      this.$api.getLunAnGroup({ dwbm: '980000', bmbm: this.form.bmbm })\r\n        .then(res => {\r\n          console.log(res)\r\n          this.lunAnZu = res.data\r\n        })\r\n    },\r\n    getTime() {\r\n      const nowDate = new Date()\r\n      const date = {\r\n        year: nowDate.getFullYear(),\r\n        month: nowDate.getMonth() + 1\r\n      }\r\n      let mouth = ''\r\n      // 月\r\n      if (date.month < 10) {\r\n        mouth = '0' + date.month\r\n      } else {\r\n        mouth = date.month\r\n      }\r\n      // 日\r\n      // if (date.date < 10) {\r\n      //     day = '0' + date.date\r\n      // } else {\r\n      //     day = date.date\r\n      // }\r\n      this.form.startDate = date.year - 1 + '-' + mouth\r\n      this.form.endDate = date.year + '-' + mouth\r\n    },\r\n    handleFilter() {\r\n      this.$api.getLunAnFind({}).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.count\r\n      })\r\n    },\r\n    getList() {\r\n      this.listLoading = false\r\n      // listBook(this.listQuery).then(response => {\r\n      //   this.tableData = response.data\r\n      //   this.list = this.tableData.list\r\n      //   this.total = this.tableData.count\r\n      //   this.listLoading = false\r\n      // })\r\n    },\r\n    handleClick(row) {\r\n      this.caseTitle = row.rymc + '检察官办案情况统计分析'\r\n      this.$api.getLunAnBaxq({ rybm: row.rybm, start: this.form.startDate, end: this.form.endDate, dwbm: '980000' }).then(res => {\r\n        console.log(res)\r\n        this.pieData = res.data.blztStatistic\r\n        const a = res.data.monthStatistic\r\n        for (var i = 0; i < a.length; i++) {\r\n          this.xAxisData.push(a[i].yearMonth)\r\n          this.barData[0].data.push(a[i].value)\r\n        }\r\n        this.caseVisible = true\r\n        this.caseShow = true\r\n      })\r\n      // 在这里面拿到后台数据渲染表格情况\r\n    },\r\n    refresh(val) {\r\n      this.$api.getLunAnFind(\r\n        { pageNum: val.page }\r\n      ).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n      })\r\n    },\r\n    displayPop() {\r\n      this.caseShow = false\r\n      this.caseVisible = false\r\n      this.pieData = []\r\n      this.barData[0].data = []\r\n      this.xAxisData = []\r\n    },\r\n    // 点击柱图\r\n    clickBar(data) {\r\n      this.$router.push({\r\n        path: 'ajlb'\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n<style lang='scss' scoped>\r\n.dashed-right-line{\r\n  border-right: 1px dashed #a9c4df;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/zhajgl/znla/laqk"}]}