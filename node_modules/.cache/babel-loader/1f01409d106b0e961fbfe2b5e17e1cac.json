    year: nowDate.getFullYear(),\r\n        month: nowDate.getMonth() + 1\r\n      }\r\n      let mouth = ''\r\n      // 月\r\n      if (date.month < 10) {\r\n        mouth = '0' + date.month\r\n      } else {\r\n        mouth = date.month\r\n      }\r\n      // 日\r\n      // if (date.date < 10) {\r\n      //     day = '0' + date.date\r\n      // } else {\r\n      //     day = date.date\r\n      // }\r\n      this.form.startDate = date.year - 1 + '-' + mouth\r\n      this.form.endDate = date.year + '-' + mouth\r\n    },\r\n    handleFilter() {\r\n      this.$api.getLunAnBllcx({ start: this.form.startDate, end: this.form.endDate, dwbm: '980000', aydm: this.form.ajbm }).then(res => {\r\n        console.log(res)\r\n        this.tableData = res.data.records\r\n        this.total = res.data.count\r\n      })\r\n    },\r\n    getList() {\r\n      this.listLoading = false\r\n      // listBook(this.listQuery).then(response => {\r\n      //   this.tableData = response.data\r\n      //   this.list = this.tableData.list\r\n      //   this.total = this.tableData.count\r\n      //   this.listLoading = false\r\n      // })\r\n    },\r\n    handleClick(row) {\r\n      this.caseVisible = true\r\n      this.caseTitle = row.name + '检察官办案情况统计分析'\r\n    },\r\n    refresh(val) {\r\n      this.$api.getLunAnBllcx({ start: this.form.startDate, end: this.form.endDate, dwbm: '980000', aydm: this.form.ajbm, pageNum: val.page })\r\n        .then(res => {\r\n          console.log(res)\r\n          this.tableData = res.data.records\r\n          this.total = res.data.count\r\n        })\r\n    },\r\n    // 点击柱图\r\n    clickBar(data) {\r\n      this.$router.push({\r\n        path: 'barMx'\r\n      })\r\n    },\r\n    gotoNext(row) {\r\n      console.log(row.id)\r\n      this.$router.push({\r\n        path: '/zhajgl/znla/cjzmbl/' + row.id\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/zhajgl/znla/cjzmbl"}]}