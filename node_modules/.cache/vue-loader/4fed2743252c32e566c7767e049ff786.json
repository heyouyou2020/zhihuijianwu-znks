log(res)\r\n        this.listLoading = false\r\n      })\r\n    },\r\n    handleFilter() {\r\n      console.log('handleFilter', this.listQuery)\r\n      this.listQuery.page = 1\r\n    },\r\n    // 列表选择\r\n    handleSelectionChange(sels) {\r\n      this.sels = sels\r\n    },\r\n    // 批量公示\r\n    batchShow() {\r\n      console.log(1)\r\n    },\r\n    // 批量删除\r\n    batchDelete() {\r\n      this.$confirm('你是否确认删除选中的检察官档案?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        customClass: 'message-manage',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功'\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    toLink(type) {\r\n      this.$router.push('ajgkznsc/details')\r\n    },\r\n    refresh() {\r\n      // this.$router.push({\r\n      //     path: '/book/list',\r\n      //     query: this.listQuery\r\n      // })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang='scss' scoped>\r\n.manageList{\r\n  .reset{\r\n    background: #ebf3fe;\r\n    color: #5c7ab0;\r\n    border-color: #ebf3fe;\r\n    &:hover{\r\n      background: #c4dcf4;\r\n      color: #5c7ab0;\r\n      border-color: #cbdbf3;\r\n    }\r\n  }\r\n  .batchOperate{\r\n    button{\r\n      padding: 8px 12px 8px 34px;\r\n      &:first-child{\r\n        background: url('../../../../icons/img/gongshi.png') no-repeat left 13px center;\r\n      }\r\n      &:last-child{\r\n        background: url('../../../../icons/img/delete.png') no-repeat left 13px center;\r\n      }\r\n    }\r\n  }\r\n  .table-hidden-cell{\r\n    .el-button--medium{\r\n      padding: 0;\r\n    }\r\n  }\r\n}\r\n</style>\r\n<style lang='scss'>\r\n.message-manage{\r\n  .el-message-box__title{\r\n    color: #4084f0;\r\n  }\r\n  .el-message-box__header{\r\n    padding-bottom: 21px;\r\n  }\r\n  .el-icon-warning:before{\r\n    content: '';\r\n    display: inline-block;\r\n    width: 52px;\r\n    height: 52px;\r\n   