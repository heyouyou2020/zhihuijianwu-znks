bleHeader\"\r\n          :key=\"item.filed\"\r\n          :prop=\"item.filed\"\r\n          :label=\"item.title\"\r\n          :width=\"item.width\"\r\n          :show-overflow-tooltip=\"true\"\r\n          :cell-class-name=\"item.colorB\"\r\n        >\r\n          <template slot-scope=\"{row}\">\r\n            <span v-if=\"item.link\" :style=\"{ color: item.color, cursor: item.link ? 'pointer' : ''}\" @click=\"gotoNext(row)\">{{ row[item.filed] }}</span>\r\n            <span v-else :style=\"{ color: item.color }\">{{ row[item.filed] }}</span>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!--分页start-->\r\n      <div class=\"pageBox\">\r\n        <pagination\r\n          v-show=\"total > 0\"\r\n          :total=\"total\"\r\n          :page.sync=\"listQuery.page\"\r\n          :limit.sync=\"listQuery.pageSize\"\r\n          @pagination=\"refresh\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <!-- 办案情况弹框 -->\r\n    <!-- <el-dialog\r\n      class=\"base-dialog\"\r\n      :title=\"caseTitle\"\r\n      :visible.sync=\"caseVisible\"\r\n      top=\"26vh\"\r\n      width=\"1200px\"\r\n    >\r\n      <div class=\"bese-card p20\">\r\n        <el-row :gutter=\"60\">\r\n          <el-col :span=\"14\" class=\"dashed-right-line\">\r\n            <p class=\"base-desc-title\"><img src=\"@/icons/img/title-arrow.png\"><span>检察官办案量按照时间维度统计</span></p>\r\n            <gradient-bar id=\"grad-bar\" :chart-data=\"barData\" :xaxis-data=\"xAxisData\" width=\"100%\" height=\"280px\" @clickBar=\"clickBar\" />\r\n          </el-col>\r\n          <el-col :span=\"10\">\r\n            <p class=\"base-desc-title\"><img src=\"@/icons/img/title-arrow.png\"><span>检察官办案量按照已办/在办维度统计</span></p>\r\n            <pie-charts id=\"pie-chart\" width=\"100%\" height=\"280px\" :pie-data=\"pieData\" unit=\"件\" />\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <div slot=\"footer\">\r\n        <el-button type=\"primary\">确定</el-button>\r\n        <e