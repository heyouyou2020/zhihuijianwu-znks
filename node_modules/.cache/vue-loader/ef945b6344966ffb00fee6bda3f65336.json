eName\" placeholder=\"请输入\" />\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button\r\n              type=\"primary\"\r\n              style=\"margin-left: 10px\"\r\n              @click=\"handleFilter\"\r\n            >\r\n              查询\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <div class=\"mb20\">\r\n        <el-button class=\"sended\" @click=\"getList(0)\">已发送</el-button>\r\n        <el-button class=\"nosended\" @click=\"getList(1)\">未发送</el-button>\r\n      </div>\r\n      <!--表格statrt-->\r\n      <el-table\r\n        v-loading=\"listLoading\"\r\n        class=\"table-hidden-cell\"\r\n        :data=\"tableData\"\r\n        stripe\r\n        highlight-current-row\r\n      >\r\n        <el-table-column\r\n          v-for=\"item in tableHeader\"\r\n          :key=\"item.filed\"\r\n          :prop=\"item.filed\"\r\n          :label=\"item.title\"\r\n          :width=\"item.width\"\r\n          :show-overflow-tooltip=\"true\"\r\n          :cell-class-name=\"item.colorB\"\r\n        >\r\n          <template slot-scope=\"{row}\">\r\n            <img v-if=\"item.warning\" :src=\"row.warn === 0 ? require('@/icons/img/warn_yellow.png') : row.warn === 1 ? require('@/icons/img/warn_red.png') : ''\" alt=\"\">\r\n            <span :style=\"{color: item.color}\">{{ row[item.filed] }}</span>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"handleClick(scope.row)\">查看</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页start-->\r\n      <div class=\"pageBox\">\r\n        <pagination\r\n          v-show=\"total > 0\"\r\n          :total=\"total\"\r\n          :page.sync=\"listQuery.page\"\r\n          :limit.sync=\"listQuery.pageSize\"\r\n          @pagination=\"refresh\"\r\n        />\r\n      <