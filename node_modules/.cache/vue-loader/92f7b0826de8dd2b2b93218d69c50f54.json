fayuan() {\r\n      this.cities = cityOptions\r\n    },\r\n    gongan() {\r\n      this.cities = gonganOptions\r\n    },\r\n    beginTalk() {\r\n      this.talkVisible = true\r\n      this.talkTitle = '远程指挥' + this.form.name\r\n    },\r\n    handleCheckAllChange(val) {\r\n      if (val) {\r\n        for (var i = 0; i < cityOptions.length; i++) {\r\n          this.checkedCities.push(cityOptions[i])\r\n        }\r\n      } else {\r\n        this.checkedCities = []\r\n      }\r\n      this.isIndeterminate = false\r\n      console.log(val)\r\n    },\r\n    handleCheckedCitiesChange(value) {\r\n      const checkedCount = value.length\r\n      this.checkAll = checkedCount === this.cities.length\r\n      this.isIndeterminate = checkedCount > 0 && checkedCount < this.cities.length\r\n    },\r\n    clickdel(index) {\r\n      this.checkedCities.splice(index, 1)\r\n      if (this.checkedCities.length === 0) {\r\n        this.checkAll = false\r\n      }\r\n    },\r\n    handleFilter() {},\r\n    displayPop() {\r\n      if (this.zhihuilist.length > 1) {\r\n        alert('最多选择一个远程指挥对象')\r\n        return false\r\n      } else {\r\n        this.form.name = this.zhihuilist[0].name\r\n        this.form.bumen = this.zhihuilist[0].sex\r\n        this.form.guanli = this.zhihuilist[0].zjlx\r\n      }\r\n      this.popupVisible = false\r\n      this.imageShow = true\r\n    },\r\n    playPop() {\r\n      this.caseVisible = false\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.zhihuilist = val\r\n    },\r\n    displayTalkPop() {\r\n      this.talkVisible = false\r\n      this.imageShow = false\r\n    },\r\n    sendTalkPop() {},\r\n  }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .tubiao{\r\n    .image-display{\r\n      position: fixed;\r\n      right: 20px;\r\n      bottom: 200px;\r\n    }\r\n      ul{\r\n          display: flex;\r\n          flex-direction: row-reverse;\r\n          li{\r\n              padding: 4px 10px;\r\n              list-style: none;\r\n       