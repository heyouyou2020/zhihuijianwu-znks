    if (response.code === 200) {\n          this.xsItemInfo = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    // 风险预警\n    getFxyj: function() {\n      this.$api.queryFxyj().then(response => {\n        if (response.code === 200) {\n          this.fxyjOption = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    fxpb: function() {\n      this.dialogFormVisible = true\n      this.getFxyj()\n    },\n    defineInfo: function(fxyj) {\n      this.dialogFormVisible = false\n      this.xsItemInfo.fxyj = fxyj.yjSelect\n      this.xsItemInfo.fxyj = fxyj.yjSelect.value\n      this.xsItemInfo.fxyjvalue = fxyj.yjSelect.label\n      this.$refs[fxyj].resetFields()\n    },\n    setXs: function() {\n      this.xsItemInfo.xszt = '2'\n      this.$confirm('是否确定将该条线索转为违法事项？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      })\n        .then(() => {\n          this.$api.updateXszwf(this.xsItemInfo).then(response => {\n            if (response.code === 200) {\n              this.$router.push('/lajdfzxy/index')\n              this.$message({\n                type: 'info',\n                message: '修改成功'\n              })\n            } else {\n              this.$message({\n                message: response.message,\n                type: 'error'\n              })\n            }\n          })\n        })\n        .catch(action => {\n          this.$message({\n            type: 'info',\n            message: '取消修改'\n          })\n        })\n    },\n    goBack() {\n      this.$router.go(-1)\n    }\n  }\n}\n</script>\n"]}]}