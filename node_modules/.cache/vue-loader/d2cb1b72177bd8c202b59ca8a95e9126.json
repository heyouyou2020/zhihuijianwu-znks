dth: 280px\"\n            :picker-options=\"pickerOptions0\"\n            @change=\"selectDate\"\n          />\n        </el-form-item>\n        <el-form-item label=\"分流方式\" prop=\"flfs\">\n          <el-select v-model=\"listQuery.flfs\" placeholder=\"请选择\" value-key=\"value\">\n            <el-option label=\"手动分流\" value=\"1\" />\n            <el-option label=\"自动分流\" value=\"0\" />\n          </el-select>\n        </el-form-item>\n        <el-form-item v-if=\"display==1\" label=\"协同状态\" prop=\"xtzt\">\n          <el-select v-model=\"listQuery.xtzt\" placeholder=\"请选择\" value-key=\"value\">\n            <el-option label=\"未协同\" value=\"0\" />\n            <el-option label=\"待接收\" value=\"1\" />\n            <el-option label=\"已接受\" value=\"2\" />\n            <el-option label=\"已拒绝\" value=\"3\" />\n          </el-select>\n        </el-form-item>\n        <el-form-item>\n          <el-button type=\"primary\" plain @click=\"restOff('listQuery')\">重置</el-button>\n          <el-button type=\"primary\" @click=\"onSearch\">查询</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n\n    <!--      表格statrt-->\n    <el-table\n      :key=\"tableKey\"\n      v-loading=\"listLoading\"\n      stripe\n      :data=\"tableData\"\n      style=\"width: 100%\"\n      :cell-style=\"changeCellStyle\"\n      highlight-current-row\n    >\n      <el-table-column\n        prop=\"ajmc\"\n        label=\"案件名称\"\n        cell-class-name=\"colorB\"\n        :show-overflow-tooltip=\"true\"\n        width=\"200\"\n      />\n      <el-table-column\n        prop=\"bmsah\"\n        label=\"部门受案号\"\n      />\n      <el-table-column\n        prop=\"cbbm\"\n        label=\"承办部门\"\n        width=\"120\"\n      />\n      <el-table-column\n        prop=\"cbdw\"\n        label=\"承办单位\"\n        width=\"140\"\n      />\n      <el-table-column\n        prop=\"wtrname\"\n        label=\"委托人\"\n        width=\"120\"\n      />\n      <el-table-column\n        prop=\"wtrq\"\n        label=\"委托日期\"\n        width=\"140\"\n      />\n      <el-table-column\n        prop=\"fllx\"\n        label=\"分流方式\"\n        width=\"120\"\n      >\n        <template slot-scope=\"scope\">\n          <span v-if=\"scope.row.fllx=='0'\">自动分流</span>\n          <span v-if=\"scope.row.fllx=='1'\">手动分流</span>\n        </template>\n      </el-table-column>\n      <el-table-column\n        v-if=\"display==1\"\n        prop=\"xtzt\"\n        label=\"协同状态\"\n        width=\"100\"\n      >\n        <template slot-scope=\"scope\">\n          <span v-for=\"item in xtztItem\" :key=\"item.name\">\n            <span v-if=\"scope.row.xtzt==item.name\" :style=\"{color: item.color}\">{{ item.text }}</span>\n          </span>\n          <!--            <span v-if=\"scope.row.fxyj=='一级预警'\" style=\"color:red\">{{ scope.row.fxyj }}</span>-->\n          <!--            <span v-if=\"scope.row.fxyj=='二级预警'\" style=\"color:blue\">{{ scope.row.fxyj }}</span>-->\n          <!--            <span v-if=\"scope.row.fxyj=='三级预警'\" style=\"color:green\">{{ scope.row.fxyj }}</span>-->\n          <!--            <span v-if=\"scope.row.fxyj=='四级预警'\" style=\"color:green\">{{ scope.row.fxyj }}</span>-->\n          <!--            <span v-if=\"scope.row.fxyj=='无风险预警'\" >{{ scope.row.fxyj }}</span>-->\n        </template>\n      </el-table-column>\n      <el-table-column\n        label=\"操作\"\n        width=\"160\"\n      >\n        <template slot-scope=\"scope\">\n          <el-button v-if=\"display==0\" type=\"text\" size=\"small\" @click=\"handleItem(scope.row)\">处理</el-button>\n          <el-button v-if=\"display==1\" type=\"text\" size=\"small\" @click=\"checkItem(scope.row)\">审查</el-button>\n          <el-button v-if=\"display==2\" type=\"text\" size=\"small\" @click=\"lookItem(scope.row)\">查看</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div class=\"pageBox\">\n      <pagination\n        v-show=\"total > 0\"\n        :total=\"total\"\n        :page.sync=\"listQuery.currentPage\"\n        :limit.sync=\"listQuery.pageSize\"\n        @pagination=\"refresh\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport Pagination from '@/components/Pagination/index'\nexport default {\n  name: 'Dashboard',\n  components: { Pagination },\n  data() {\n    return {\n      listQuery: {\n        status: 2,\n        userid: 1,\n        pageSize: 10,\n        currentPage: 1\n      },\n      laqkOption: [],\n      fxyjOption: [],\n      tableKey: 0,\n      tableData: [],\n      listLoading: true,\n      total: 100,\n      display: 0,\n      xtztItem: [\n        {\n          name: '2',\n          text: '已接受',\n          color: '#00CC00'\n        },\n        {\n          name: '3',\n          text: '已拒绝',\n          color: '#FF0000'\n        },\n        {\n          name: '0',\n          text: '未协同',\n          color: '#999999'\n        },\n        {\n          name: '1',\n          text: '待接收',\n          color: '#999999'\n        }\n      ],\n      pickerOptions0: {\n        disabledDate(time) {\n          return time.getTime() > Date.now()\n        }\n      }\n    }\n  },\n  mounted() {\n    const curBtn = localStorage.getItem('curBtn')\n    if (curBtn) {\n      this.display = curBtn\n    }\n    this.getList()\n  },\n  methods: {\n    displayList(flat) {\n      this.display = flat\n      this.listQuery.currentPage = 1\n      localStorage.setItem('curBtn', flat)\n      this.getList(flat)\n    },\n    selectDate: function() {\n      const dateTime = this.listQuery.wtrq\n      if (dateTime == null) {\n        this.listQuery.wtkssj = ''\n        this.listQuery.wtjssj = ''\n      } else {\n        const startTime = this.moment(dateTime[0]).format('YYYY-MM-DD')\n        const endTime = this.moment(dateTime[1]).format('YYYY-MM-DD')\n        this.listQuery.wtkssj = startTime\n        this.listQuery.wtjssj = endTime\n      }\n    },\n    onSearch: function() {\n      this.listQuery.currentPage = 1\n      this.getList()\n    },\n    restOff: function(listQuery) {\n      this.$refs[listQuery].resetFields()\n      this.listQuery.wtkssj = ''\n      this.listQuery.wtjssj = ''\n      this.getList()\n    },\n    changeCellStyle({ row, columnIndex }) {\n      if (columnIndex === 0) {\n        return 'color: #4084ef'\n      }\n    },\n    getList() {\n      this.listLoading = true// true\n      this.listQuery.jszt = this.display\n      this.$api.scwtPageList(this.listQuery).then(response => {\n        if (response.code === 200) {\n          this.tableData = response.data.list\n          this.total = response.data.total\n          this.listLoading = false\n        } else {\n          this.$message({\n            message: response.message,\n            type: 'warning'\n          })\n        }\n      })\n    },\n    checkItem: function(item) {\n      this.$router.push({ path: '/znsc/scyj', query: { xsInfoId: item.id }})\n    },\n    handleItem: function(item) {\n      this.$router.push({ path: '/znsc/clsc', query: { xsInfoId: item.id, zt: 0 }})\n    },\n    lookItem: function(item) {\n      this.$router.push({ path: '/znsc/clsc', query: { xsInfoId: item.id, zt: 1 }})\n    },\n    refresh() {\n      this.getList()\n    }\n  }\n}\n</script>\n<style>\n    .active{\n        color: #fff !important;\n        background: #78a2cc !important;\n        border:none !important;\n    }\n</style>\n"]}]}