oup v-model=\"radio\">\n              <el-radio :label=\"0\">自动分流</el-radio>\n              <el-radio :label=\"1\">手动分流</el-radio>\n            </el-radio-group>\n          </el-form-item>\n        </el-row>\n        <div v-if=\"radio==0\" class=\"showCenter\">\n          系统自动分流到检察技术处：刘忠奇\n        </div>\n        <div v-if=\"radio==1\">\n          <p class=\"base-border-title mb15\">案件分配原因</p>\n          <el-form-item>\n            <el-radio-group v-model=\"xsItemInfo.fpyy\" class=\"item-pd\">\n              <el-radio v-for=\"item in assReason\" :key=\"item.value\" :label=\"item.value\">{{ item.label }}</el-radio>\n            </el-radio-group>\n          </el-form-item>\n          <p class=\"base-border-title mtb20\">案件分配情况</p>\n          <div class=\"item-pd\">\n            <el-form :inline=\"true\">\n              <el-form-item label=\"分流部门:\">\n                <!--                <el-input-->\n                <!--                  v-model=\"xsItemInfo.flbm\"-->\n                <!--                  placeholder=\"检察技术处\"-->\n                <!--                  clearable-->\n                <!--                  class=\"fl\"-->\n                <!--                  readonly-->\n                <!--                  value=\"3700000020\"-->\n                <!--                />-->\n                <el-select v-model=\"xsItemInfo.flbm\" placeholder=\"检察技术处\" value-key=\"id\" @change=\"selectPerson\">\n                  <el-option\n                    v-for=\"item in fpbmption\"\n                    :key=\"item.id\"\n                    :label=\"item.name\"\n                    :value=\"item.id\"\n                  />\n                </el-select>\n              </el-form-item>\n            </el-form>\n          </div>\n          <div class=\"mt15\">\n            <table class=\"renderTable renderTable2\">\n              <thead>\n                <th style=\"width: 130px;\">序号</th>\n                <th style=\"width: 160px;\">姓名</th>\n                <th style=\"width: 160px;\">是否在位</th>\n                <th style=\"width: 106px;\">轮案规则</th>\n                <th>选择轮案人</th>\n              </thead>\n              <tbody v-if=\"fpryList.length>0\">\n                <tr v-for=\"(item,index) in fpryList\" :key=\"item.id\">\n                  <td>{{ index+1 }}</td>\n                  <td>{{ item.username }}</td>\n                  <td>是</td>\n                  <td>1/1</td>\n                  <td>\n                    <el-radio v-model=\"xsItemInfo.scrid\" :label=\"item.id\">{{ '' }}</el-radio>\n                  </td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </el-form>\n      <el-row>\n        <div class=\"mt50 fr\">\n          <el-button type=\"primary\" @click=\"choseQD\">确 定</el-button>\n          <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"$router.back(-1)\">取消</el-button>\n        </div>\n      </el-row>\n    </div>\n\n  </div>\n</template>\n\n<script>\n\nexport default {\n\n  data() {\n    return {\n      xsbh: '',\n      xsItemInfo: {\n        wtid: '',\n        glyid: JSON.parse(window.localStorage.getItem('userInfo')).id,\n        flbm: '3700000020'\n      },\n      radio: 0,\n      assReason: [\n        { label: '未成年犯罪', value: 0 },\n        { label: '提前介入的案件', value: 1 },\n        { label: '领导指定', value: 2 },\n        { label: '复议', value: 3 },\n        { label: '备案案件', value: 4 },\n        { label: '商情指定管辖案件', value: 5 },\n        { label: '追诉同案犯的案件', value: 6 },\n        { label: '其他', value: 7 }\n      ],\n      fpbmption: [],\n      fpryList: []\n    }\n  },\n  watch: {\n    radio() {\n      if (this.radio === 1) {\n        this.getCode('ajfpyy')\n        this.getFpbm()\n        this.getUserByDwbm()\n      }\n    }\n  },\n  mounted() {\n    this.xsbh = this.$route.query.xsInfoId\n    this.getXsInfo(this.xsbh)\n    // this.getUserByDwbm()\n  },\n  methods: {\n    getXsInfo(xsbh) {\n      const params = {\n        id: this.xsbh\n      }\n      this.$api.getScwtById(params).then(response => {\n        if (response.code === 200) {\n          this.xsItemInfo = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    getCode(node) {\n      this.$api.getCode(node).then(response => {\n        if (response.code === 200) {\n          this.assReason = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    getFpbm() {\n      this.$api.getFlDept().then(response => {\n        if (response.code === 200) {\n          this.fpbmption = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    selectPerson(val) {\n      this.getUserByDwbm()\n    },\n    getUserByDwbm() {\n      const params = {\n        dwbm: this.xsItemInfo.flbm || '3700000020',\n        wtid: this.xsbh,\n        userid: JSON.parse(window.localStorage.getItem('userInfo')).id\n      }\n      this.$api.getUserByDwbm(params).then(response => {\n        if (response.code === 200) {\n          this.fpryList = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    choseQD: function() {\n      const params = {\n        fllx: this.radio,\n        fpyy: this.xsItemInfo.fpyy || '',\n        wtid: this.xsbh,\n        glyid: JSON.parse(window.localStorage.getItem('userInfo')).id,\n        scrid: this.xsItemInfo.scrid || '',\n        flbm: this.xsItemInfo.flbm || '3700000020'\n      }\n      // this.xsForm = Object.assign(this.xsItemInfo, this.xgaj)\n      this.$confirm('是否确定将该条案件进行分流？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      })\n        .then(() => {\n          this.$api.addAjfl(params).then(response => {\n            if (response.code === 200) {\n              this.$router.push('/scgly')\n              this.$message({\n                type: 'info',\n                message: '修改成功'\n              })\n            } else {\n              this.$message({\n                message: response.message,\n                type: 'error'\n              })\n            }\n          })\n        })\n        .catch(action => {\n          this.$message({\n            type: 'info',\n            message: '取消修改'\n          })\n        })\n    }\n  }\n}\n</script>\n<style lang='scss' scoped>\n .flbox{\n     /deep/.el-radio__label{\n         color: #6e87b8;\n     }\n     /deep/.el-input--medium .el-input__inner{\n         color: #879bba;\n     }\n     .renderTable2{\n         td:nth-child(odd){\n             width: auto;\n         }\n         td:nth-child(even) {\n             width: auto;\n         }\n         th, td{\n             padding: 0 10px;\n             border: 1px solid #a9c4df;\n             height: 36px;\n             line-height: 36px;\n             text-align: left;\n         }\n     }\n     .showCenter{\n         display: flex;\n         justify-content: center;\n         align-items: center;\n         width: 100%;\n         height: 200px;\n         background-color: #efefef;\n     }\n }\n</style>\n"]}]}