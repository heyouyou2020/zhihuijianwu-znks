tr>\n              <td>委托要求：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.wtyq }}\n              </td>\n            </tr>\n            <tr>\n              <td>资料上传：</td>\n              <td colspan=\"3\">\n                <ul v-if=\"zlList.length>0\" class=\"uploadShow\">\n                  <li v-for=\"(item,i) in zlList\" :key=\"i\">\n                    <span class=\"fr ml10\" style=\"cursor: pointer\" @click=\"down(item)\"><img src=\"@/icons/img/down.png\" height=\"12\" width=\"12\"></span>\n                    <span v-if=\"item.type=='pic'\"><img src=\"@/icons/img/pic.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else-if=\"item.type=='audio'\"><img src=\"@/icons/img/audio.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else-if=\"item.type=='video'\"><img src=\"@/icons/img/video.png\" height=\"16\" width=\"14\"></span>\n                    <span v-else><img src=\"@/icons/img/wd.png\" height=\"16\" width=\"14\"></span>\n                    <span>{{ item.name }}</span>\n                  </li>\n                </ul>\n              </td>\n            </tr>\n            <tr>\n              <td>备注：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.bz }}\n              </td>\n            </tr>\n            <tr v-if=\"zt==0\">\n              <td>协同原因：</td>\n              <td colspan=\"3\">\n                {{ xsItemInfo.xtyy }}\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </el-form>\n      <el-row>\n        <div v-if=\"zt==0\" class=\"mt50 fr\">\n          <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"$router.back(-1)\">取消</el-button>\n        </div>\n        <div v-if=\"zt==1\" class=\"mt50 fr\">\n          <el-button type=\"primary\" @click=\"agree\">同意</el-button>\n          <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"reject\">拒绝</el-button>\n        </div>\n      </el-row>\n\n    </div>\n  </div>\n</t