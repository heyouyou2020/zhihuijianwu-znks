nfo')).id,\n        flid: this.xsItemInfo.flid\n      }\n      this.$confirm('确认拒绝此审查委托吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$api.updateAjflXtzt(param).then(response => {\n          if (response.code === 200) {\n            this.$message({\n              type: 'success',\n              message: '操作成功!'\n            })\n            this.$router.push('/xtsc')\n          }\n        })\n      }).catch(() => {\n        // this.$message({\n        //   type: 'info',\n        //   message: '已取消删除'\n        // })\n      })\n    }\n  }\n}\n</script>\n\n"]}]}