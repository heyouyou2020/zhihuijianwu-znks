nput\n              v-model=\"listQuery.wjmc\"\n              placeholder=\"请输入关键字\"\n              class=\"filter-item\"\n              clearable\n            />\n          </el-col>\n          <el-col :span=\"4\">\n            <el-button\n              class=\"filter-item\"\n              type=\"primary\"\n              style=\"margin-left: 10px\"\n              @click=\"handleFilter\"\n            >\n              查询\n            </el-button>\n          </el-col>\n        </el-row>\n      </div>\n      <div class=\"listFl\">\n        <div class=\"listUl\">\n          <ul v-if=\"flyjList.length>0\" class=\"clearFix\">\n            <li v-for=\"item in flyjList\" :key=\"item.id\" @click=\"lookInfo(item.id)\">\n              <p class=\"text-ellipsis\">{{ item.wjmc }}</p>\n              <div>法律 <el-divider direction=\"vertical\" />全国人大常委会 <el-divider direction=\"vertical\" />\n                2019-08-26发布 <el-divider direction=\"vertical\" />2019-12-12实施</div>\n            </li>\n          </ul>\n          <div v-if=\"flyjList.length==0\" class=\"noSj\">暂无数据</div>\n        </div>\n        <div class=\"flWrapper\">\n          <p>您可以实试试搜索：</p>\n          <div class=\"text-ellipsis\">婚姻法</div>\n          <div class=\"text-ellipsis\">劳动合同法</div>\n          <div class=\"text-ellipsis\">刑法第293条</div>\n          <div class=\"text-ellipsis\">民事诉讼法司法解释</div>\n        </div>\n      </div>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\">确 定</el-button>\n        <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"onClose\">取 消</el-button>\n      </div>\n      <div class=\"pageBox\">\n        <pagination\n          v-show=\"total > 0\"\n          :total=\"total\"\n          :page.sync=\"listQuery.currentPage\"\n          :limit.sync=\"listQuery.pageSize\"\n          @pagination=\"refresh\"\n        />\n      </div>\n    </el-dialog>\n    <!--      查看详情弹出框-->\n    <el-dialog title=\"查看法律法规\" :visible.sync=\"dialogVisibleInfo\" class=\"dialog-render\" width=\"50%\">\n      <h2 style=\"text-align: center\">{{ flnr.wjmc }}</h2>\n      <div class=\"baText\" v-html=\"flnr.wjnr\" />\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"dialogVisibleInfo=false\">确 定</el-button>\n        <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"dialogVisibleInfo=false\">取 消</el-button>\n      </div>\n    </el-dialog>\n    <!--    办案指引弹出框-->\n    <el-dialog title=\"办案指引\" :visible.sync=\"dialogVisible2\" class=\"dialog-render\" width=\"50%\" :before-close=\"onClose2\" :show-id=\"lbID\">\n      <div>\n        <h2 style=\"text-align: center\">{{ bazyInfo.mc }}</h2>\n        <div class=\"baText\" v-html=\"bazyInfo.nr\" />\n      </div>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\">确 定</el-button>\n        <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"onClose2\">取 消</el-button>\n      </div>\n    </el-dialog>\n  <!--    研判预测弹出框-->\n    <!--  <el-dialog title=\"研判预测\" :visible.sync=\"dialogYpyc\" class=\"dialog-render\" width=\"65%\" />-->\n  </div>\n</template>\n<script>\nimport Pagination from '@/components/Pagination/index'\nexport default {\n  components: { Pagination },\n  props: ['dialogVisible', 'dialogVisible2', 'lbID'],\n  data() {\n    return {\n      listQuery: {\n        wjmc: '',\n        currentPage: 1,\n        pageSize: 10\n      },\n      total: 0,\n      flyjList: [],\n      bazyInfo: '',\n      dialogVisibleInfo: false,\n      flnr: {}\n    }\n  },\n  watch: {\n    lbID: function(a, b) {\n\n      // this.getflyj(a)\n    }\n  },\n  mounted() {\n    this.getflyj()// 法律依据列表\n    this.getBazy(this.lbID)// 办案指引\n  },\n  methods: {\n    onClose() {\n      // this.dialogVisible = false\n      this.$emit('update:dialogVisible', false)\n    },\n    onClose2() {\n      // this.dialogVisible = false\n      this.$emit('update:dialogVisible2', false)\n    },\n    handleFilter: function() {\n      this.listQuery.currentPage = 1\n      this.getflyj(this.listQuery)\n    },\n    getflyj: function() {\n      this.$api.queryFlyj(this.listQuery).then(response => {\n        if (response.code === 200) {\n          this.flyjList = response.data.list\n          this.total = response.data.total\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    getBazy: function(a) {\n      this.$api.queryBazy(a).then(response => {\n        if (response.code === 200) {\n          this.bazyInfo = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    },\n    refresh() {\n      this.getflyj()\n    },\n    lookInfo(id) {\n      this.dialogVisibleInfo = true\n      this.$api.queryFvxxbyid(id).then(response => {\n        if (response.code === 200) {\n          this.flnr = response.data\n        } else {\n          this.$message.error(response.message)\n        }\n      })\n    }\n  }\n}\n</script>\n<style  lang=\"scss\" scoped>\n  .noSj{\n    text-align: center;\n    margin-top: 40px;\n    font-size: 18px;\n  }\n  .listFl{\n    display: flex;\n    padding-bottom: 52px;\n      .listUl{\n        flex: 1;\n        width: 500px;\n          li{\n            border: 1px solid #a9c4df;\n            padding: 15px 10px;\n            padding-bottom: 10px;\n            color: #4f5e7b;\n            font-size: 16px;\n            margin-bottom: 10px;\n            p{\n              font-weight: bold;\n            }\n            div{\n              color: #8c97ab;\n              line-height: 32px;\n            }\n          }\n      }\n    .flWrapper{\n      width: 160px;\n      color: #4f5e7b;\n      font-size: 16px;\n      margin-left: 30px;\n      p{\n        font-weight: bold;\n      }\n      div{\n        margin-top: 15px;\n        color: #8c97ab;\n      }\n    }\n  }\n  .baText{\n    font-size: 16px;\n    line-height: 28px;\n    text-indent: 2em;\n  }\n</style>\n"]}]}