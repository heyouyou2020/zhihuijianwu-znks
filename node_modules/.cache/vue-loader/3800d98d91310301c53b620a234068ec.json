mplate>\r\n  <div class=\"dashboard-container manageList\">\r\n    <div class=\" clearfix\">\r\n      <!-- 头部搜索start-->\r\n      <div class=\"\">\r\n        <el-form class=\"base-search-form\" :model=\"listQuery\" :inline=\"true\">\r\n          <el-form-item label=\"检察官姓名\">\r\n            <el-input v-model=\"listQuery.inquisitorName\" placeholder=\"请输入\" />\r\n          </el-form-item>\r\n          <el-form-item label=\"档案年度\">\r\n            <el-select v-model=\"listQuery.archivesYear\" placeholder=\"请选择\">\r\n              <el-option\r\n                v-for=\"item in yearList\"\r\n                :key=\"item.archivesYear\"\r\n                :label=\"item.archivesName\"\r\n                :value=\"item.archivesYear\"\r\n              />\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"所属部门\">\r\n            <el-select v-model=\"listQuery.deptName\" placeholder=\"请选择\">\r\n              <el-option\r\n                v-for=\"item in deptList\"\r\n                :key=\"item.bmbm\"\r\n                :label=\"item.bmmc\"\r\n                :value=\"item.bmbm\"\r\n              />\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"档案状态\">\r\n            <el-select v-model=\"listQuery.archivesStatus\" placeholder=\"请选择\">\r\n              <el-option label=\"未公示\" value=\"0\" />\r\n              <el-option label=\"已公示\" value=\"1\" />\r\n              <el-option label=\"已封存\" value=\"2\" />\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button\r\n              class=\"reset\"\r\n              style=\"margin-left: 10px\"\r\n              @click=\"handleFilter\"\r\n            >\r\n              重置\r\n            </el-button>\r\n            <el-button\r\n              type=\"primary\"\r\n              style=\"margin-left: 10px\"\r\n              @click=\"handleFilter\"\r\n            >\r\n              查询\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <div class=\"mb20 fr batchOperate\">\r\n        <el-button @click=\"dialogFormVisible = true\">添加司法档案</el-button>\r\n        <el-button @click=\"batchDelete\">批量删除</el-button>\r\n      </div>\r\n      <!--表格statrt-->\r\n      <el-table\r\n        v-loading=\"listLoading\"\r\n        class=\"table-hidden-cell\"\r\n        :data=\"tableData\"\r\n        stripe\r\n        highlight-current-row\r\n        @selection-change=\"handleSelectionChange\"\r\n      >\r\n        <el-table-column\r\n          type=\"selection\"\r\n          width=\"55\"\r\n        />\r\n        <el-table-column\r\n          prop=\"inquisitorName\"\r\n          label=\"检察官姓名\"\r\n          :show-overflow-tooltip=\"true\"\r\n        />\r\n        <el-table-column\r\n          prop=\"archivesName\"\r\n          label=\"档案名称\"\r\n          :show-overflow-tooltip=\"true\"\r\n        />\r\n        <el-table-column\r\n          prop=\"rdjdrq\"\r\n          label=\"入档阶段\"\r\n          :show-overflow-tooltip=\"true\"\r\n        />\r\n        <el-table-column\r\n          prop=\"position\"\r\n          label=\"职务\"\r\n          :show-overflow-tooltip=\"true\"\r\n        />\r\n        <el-table-column\r\n          prop=\"dept\"\r\n          label=\"所属部门\"\r\n          :show-overflow-tooltip=\"true\"\r\n        />\r\n        <el-table-column\r\n          prop=\"archivesStatus\"\r\n          label=\"档案状态\"\r\n          :show-overflow-tooltip=\"true\"\r\n        />\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"toLink(0)\">查看</el-button>\r\n            <el-button type=\"text\" @click=\"toLink(1)\">编辑</el-button>\r\n            <el-button type=\"text\" @click=\"batchDelete(scope.row.id)\">删除</el-button>\r\n            <el-button type=\"text\" @click=\"handleClick(scope.row)\">撤销</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页start-->\r\n      <div class=\"pageBox\">\r\n        <pagination\r\n          v-show=\"total > 0\"\r\n          :total=\"total\"\r\n          :page.sync=\"listQuery.page\"\r\n          :limit.sync=\"listQuery.pageSize\"\r\n          @pagination=\"refresh\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <!--弹出框-->\r\n    <el-dialog title=\"建立司法档案\" :visible.sync=\"dialogFormVisible\" class=\"dialog-render\" width=\"800px\">\r\n      <el-form ref=\"form\" :model=\"formData\" :rules=\"rules\" label-width=\"190px\">\r\n        <el-row>\r\n          <el-col :span=\"12\">\r\n            <el-form-item prop=\"archivesYear\" label=\"请选择司法档案填写年度：\">\r\n              <el-select v-model=\"formData.archivesYear\" placeholder=\"请选择\" @change=\"optYear\">\r\n                <el-option\r\n                  v-for=\"item in yearList\"\r\n                  :key=\"item.archivesYear\"\r\n                  :label=\"item.archivesName\"\r\n                  :value=\"item.archivesYear\"\r\n                />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item prop=\"archivesName\" label=\"档案名称：\">\r\n              <el-input v-model=\"formData.archivesName\" />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span=\"24\">\r\n            <el-form-item label=\"入档阶段：\">\r\n              <el-date-picker\r\n                v-model=\"timeDate\"\r\n                readonly=\"readonly\"\r\n                format=\"yyyy/MM\"\r\n                format-value=\"yyyy/MM\"\r\n                type=\"monthrange\"\r\n                range-separator=\"至\"\r\n                start-placeholder=\"开始月份\"\r\n                end-placeholder=\"结束月份\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n        <el-row>\r\n          <el-col :span=\"12\">\r\n            <el-form-item prop=\"dept\" label=\"请选择部门：\">\r\n              <el-select v-model=\"formData.dept\" placeholder=\"请选择\" @change=\"optDept\">\r\n                <el-option\r\n                  v-for=\"item in deptList\"\r\n                  :key=\"item.bmbm\"\r\n                  :label=\"item.bmmc\"\r\n                  :value=\"item.bmbm\"\r\n                />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-form-item prop=\"inquisitorName\" label=\"请选择检察官：\">\r\n              <el-select v-model=\"formData.inquisitorName\" placeholder=\"请选择\">\r\n                <el-option\r\n                  v-for=\"(item, index) in inquisitorList\"\r\n                  :key=\"item.id + index\"\r\n                  :label=\"item.inquisitorName\"\r\n                  :value=\"item.id\"\r\n                />\r\n              </el-select>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"addDA\">确 定</el-button>\r\n        <el-button type=\"primary\" plain class=\"colorPlain\" @click=\"dialogFormVisible = false\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Pagination from '@/components/Pagination/index'\r\nexport default {\r\n  name: 'SendCase',\r\n  components: { Pagination },\r\n  data() {\r\n    return {\r\n      dialogFormVisible: false,\r\n      dwbm: '980000',\r\n      yearList: [],\r\n      deptList: [],\r\n      inquisitorList: [],\r\n      listQuery: {\r\n        pageNum: 1,\r\n        size: 20,\r\n        archivesStatus: '',\r\n        deptName: '',\r\n        archivesYear: '',\r\n        inquisitorName: '',\r\n        pgType: 0\r\n      },\r\n      tableData: [],\r\n      formData: {\r\n        archivesYear: '',\r\n        archivesName: '',\r\n        rdjdrq: '',\r\n        dept: '',\r\n        inquisitorName: ''\r\n      },\r\n      timeDate: [],\r\n      listLoading: false,\r\n      show: false,\r\n      total: 100,\r\n      rules: {\r\n        archivesYear: [\r\n          { required: true, message: '请选择司法档案填写年度', trigger: 'change' }\r\n        ],\r\n        archivesName: [\r\n          { required: true, message: '请输入档案名称', trigger: 'blur' }\r\n        ],\r\n        dept: [\r\n          { required: true, message: '请选择部门', trigger: 'change' }\r\n        ],\r\n        inquisitorName: [\r\n          { required: true, message: '请选择检察官', trigger: 'change' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n  },\r\n  mounted() {\r\n    // 获取年份列表\r\n    this.getYearList()\r\n    // 查询列表\r\n    this.getInquisitorInfoList()\r\n    // 获取部门列表\r\n    this.getDept()\r\n  },\r\n  methods: {\r\n    // 获取年份列表\r\n    getYearList() {\r\n      this.$api.getYearList().then(res => {\r\n        if (res.code === 0) {\r\n          this.yearList = res.data\r\n        }\r\n      })\r\n    },\r\n    // 选择填写年度\r\n    optYear(val) {\r\n      this.rdjdrq = val\r\n      this.timeDate = val.split('-')\r\n      console.log()\r\n    },\r\n    // 查询档案列表\r\n    getInquisitorInfoList() {\r\n      this.$api.getDAList(this.listQuery).then(res => {\r\n        if (res.code === 0) {\r\n          this.tableData = res.data.records\r\n          this.total = res.data.count\r\n        }\r\n      })\r\n    },\r\n    // 获取部门列表\r\n    getDept() {\r\n      this.$api.getDept(this.dwbm).then(res => {\r\n        if (res.code === 0) {\r\n          this.deptList = res.data\r\n        }\r\n      })\r\n    },\r\n    // 选择部门\r\n    optDept() {\r\n      this.getInquisitor()\r\n    },\r\n    // 获取检察官\r\n    getInquisitor() {\r\n      const params = {\r\n        dwbm: this.dwbm,\r\n        bmbm: this.formData.dept\r\n      }\r\n      this.$api.getInquisitor(params).then(res => {\r\n        if (res.code === 0) {\r\n          this.inquisitorList = res.data\r\n        }\r\n      })\r\n    },\r\n    // 添加档案\r\n    addDA() {\r\n      this.$refs.form.validate((valid) => {\r\n        if (valid) {\r\n          console.log(this.formData)\r\n          this.$router.push({ path: 'entry/details', query: this.formData })\r\n        }\r\n      })\r\n    },\r\n    handleFilter() {\r\n      console.log('handleFilter', this.listQuery)\r\n      this.listQuery.page = 1\r\n    },\r\n    // 列表选择\r\n    handleSelectionChange(sels) {\r\n      this.sels = sels\r\n    },\r\n    // 批量公示\r\n    batchShow() {\r\n      console.log(1)\r\n    },\r\n    // 批量删除\r\n    batchDelete() {\r\n      if (this.sels && this.sels.length > 0) {\r\n        this.deleteRow(this.sels)\r\n      } else {\r\n        this.$message({\r\n          message: '请选择删除的检察官档案'\r\n        })\r\n      }\r\n    },\r\n    deleteRow(id) {\r\n      this.$confirm('你是否确认删除选中的检察官档案?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        customClass: 'message-manage',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$api.getInquisitor(id).then(res => {\r\n          if (res.code === 0) {\r\n            this.inquisitorList = res.data\r\n          }\r\n        })\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功'\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    getList() {\r\n      this.listLoading = false\r\n    },\r\n    toLink(type) {\r\n      this.$router.push('entry/details')\r\n    },\r\n    refresh() {\r\n      // this.$router.push({\r\n      //     path: '/book/list',\r\n      //     query: this.listQuery\r\n      // })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang='scss' scoped>\r\n.manageList{\r\n  .reset{\r\n    background: #ebf3fe;\r\n    color: #5c7ab0;\r\n    border-color: #ebf3fe;\r\n    &:hover{\r\n      background: #c4dcf4;\r\n      color: #5c7ab0;\r\n      border-color: #cbdbf3;\r\n    }\r\n  }\r\n  .batchOperate{\r\n    button{\r\n      padding: 8px 12px 8px 34px;\r\n      &:first-child{\r\n        background: url('../../../../icons/img/add.png') no-repeat left 13px center;\r\n      }\r\n      &:last-child{\r\n        background: url('../../../../icons/img/delete.png') no-repeat left 13px center;\r\n      }\r\n    }\r\n  }\r\n  .table-hidden-cell{\r\n    .el-button--medium{\r\n      padding: 0;\r\n    }\r\n  }\r\n}\r\n</style>\r\n<style lang='scss'>\r\n.message-manage{\r\n  .el-message-box__title{\r\n    color: #4084f0;\r\n  }\r\n  .el-message-box__header{\r\n    padding-bottom: 21px;\r\n  }\r\n  .el-icon-warning:before{\r\n    content: '';\r\n    display: inline-block;\r\n    width: 52px;\r\n    height: 52px;\r\n    background: url('../../../../icons/img/tishi.png') no-repeat;\r\n    background-size: 100%;\r\n  }\r\n  .el-message-box__status+.el-message-box__message{\r\n    padding-left: 66px;\r\n  }\r\n  .el-message-box__btns{\r\n    button{\r\n      width: 98px;\r\n      height: 34px;\r\n      float: right;\r\n      &:first-child{\r\n        margin-left: 10px;\r\n        color: #5c7ab0;\r\n        background: #c4dcf4;\r\n        &:hover{\r\n          background: #cce4fc;\r\n        }\r\n      }\r\n      span{\r\n        font-size: 16px;\r\n      }\r\n    }\r\n  }\r\n}\r\n.manageList{\r\n  .el-form-item{\r\n    margin-bottom: 20px;\r\n  }\r\n  .el-date-editor{\r\n    width: 100%;\r\n  }\r\n  .dialog-render .el-dialog__body{\r\n    padding: 30px 20px;\r\n  }\r\n  .el-form-item__error{\r\n    top: 100% !important;\r\n  }\r\n}\r\n</style>\r\n"]}]}