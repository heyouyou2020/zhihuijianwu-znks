template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"toLink(0)\">查看</el-button>\r\n            <el-button type=\"text\" @click=\"toLink(1)\">编辑</el-button>\r\n            <el-button type=\"text\" @click=\"handleClick(scope.row)\">删除</el-button>\r\n            <el-button type=\"text\" @click=\"handleClick(scope.row)\">公示</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <!-- 分页start-->\r\n      <div class=\"pageBox\">\r\n        <pagination\r\n          v-show=\"total > 0\"\r\n          :total=\"total\"\r\n          :page.sync=\"listQuery.page\"\r\n          :limit.sync=\"listQuery.pageSize\"\r\n          @pagination=\"refresh\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Pagination from '@/components/Pagination/index'\r\nexport default {\r\n  name: 'SendCase',\r\n  components: { Pagination },\r\n  data() {\r\n    return {\r\n      yearList: [],\r\n      listQuery: {\r\n        positionName: '',\r\n        page: 1,\r\n        pageSize: 20,\r\n        keyWord: ''\r\n      },\r\n      tableData: [\r\n      ],\r\n      total: 100,\r\n      listLoading: true\r\n    }\r\n  },\r\n  created() {\r\n  },\r\n  mounted() {\r\n    this.getYearList()\r\n  },\r\n  methods: {\r\n    // 获取年份列表\r\n    getYearList() {\r\n      this.$api.getYearList(this.listQuery).then(res => {\r\n        console.log(res)\r\n      })\r\n    },\r\n    // 获取检察官列表\r\n    getList() {\r\n      this.$api.getDAList(this.listQuery).then(res => {\r\n        console.log(res)\r\n        this.listLoading = false\r\n      })\r\n    },\r\n    handleFilter() {\r\n      console.log('handleFilter', this.listQuery)\r\n      this.listQuery.page = 1\r\n    },\r\n    // 列表选择\r\n    handleSelectionChange(sels) {\r\n      this.sels = sels\r\n    },\r\n    // 批量公示\r\n    batchShow() {\r\n      console.log(1)\r\n    },\r\n    // 批量删除\r\n    batchDelete() {\r\n      this.$confirm('你是否确认删除选中的检察官档案?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        customClass: 'message-manage',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功'\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    toLink(type) {\r\n      this.$router.push('admin/details')\r\n    },\r\n    refresh() {\r\n      // this.$router.push({\r\n      //     path: '/book/list',\r\n      //     query: this.listQuery\r\n      // })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style lang='scss' scoped>\r\n.manageList{\r\n  .reset{\r\n    background: #ebf3fe;\r\n    color: #5c7ab0;\r\n    border-color: #ebf3fe;\r\n    &:hover{\r\n      background: #c4dcf4;\r\n      color: #5c7ab0;\r\n      border-color: #cbdbf3;\r\n    }\r\n  }\r\n  .batchOperate{\r\n    button{\r\n      padding: 8px 12px 8px 34px;\r\n      &:first-child{\r\n        background: url('../../../../icons/img/gongshi.png') no-repeat left 13px center;\r\n      }\r\n      &:last-child{\r\n        background: url('../../../../icons/img/delete.png') no-repeat left 13px center;\r\n      }\r\n    }\r\n  }\r\n  .table-hidden-cell{\r\n    .el-button--medium{\r\n      padding: 0;\r\n    }\r\n  }\r\n}\r\n</style>\r\n<style lang='scss'>\r\n.message-manage{\r\n  .el-message-box__title{\r\n    color: #4084f0;\r\n  }\r\n  .el-message-box__header{\r\n    padding-bottom: 21px;\r\n  }\r\n  .el-icon-warning:before{\r\n    content: '';\r\n    display: inline-block;\r\n    width: 52px;\r\n    height: 52px;\r\n    background: url('../../../../icons/img/tishi.png') no-repeat;\r\n    background-size: 100%;\r\n  }\r\n  .el-message-box__status+.el-message-box__message{\r\n    padding-left: 66px;\r\n  }\r\n  .el-message-box__btns{\r\n    button{\r\n      width: 98px;\r\n      height: 34px;\r\n      float: right;\r\n      &:first-child{\r\n        margin-left: 10px;\r\n        color: #5c7ab0;\r\n        background: #c4dcf4;\r\n        &:hover